{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "quotes": [1, "double", { "avoidEscape": true }],
        "linebreak-style": ["error", "windows"],
        
    }
}
